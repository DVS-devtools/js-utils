{
  "name": "@docomodigital/js-browser",
  "version": "1.0.1",
  "description": "JsBrowser provides you an alternative method to manage previous and current page changements.",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build:umd": "NODE_ENV=production BUILD_TARGET=umd node ../../scripts/build.js",
    "build:browser": "NODE_ENV=production BUILD_TARGET=browser node ../../scripts/build.js",
    "build": "npm run build:umd && npm run build:browser",
    "start": "NODE_ENV=development node ../../scripts/start.js",
    "test": "node ../../scripts/runTests.js",
    "test:watch": "node ../../scripts/runTests.js --watchAll",
    "test:open": "node ../../scripts/runTests.js && open coverage/lcov-report/index.html",
    "lint": "node ../../scripts/lint.js",
    "doc": "node ../../scripts/docs.js",
    "doc:latest": "node ../../scripts/docs  --version=latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
